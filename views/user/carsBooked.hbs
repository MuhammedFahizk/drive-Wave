                       <div class="col-md-4 ">
                      <div class="car-wrap rounded ftco-animate">
                        
                         <div class="card">
                <div class="card-body">
                  <div class="d-flex flex-column align-items-center text-center">
                    <div class="mt-3 mb-4">
                      <h4>{{this.pickDate}}</h4>
                      <p class="text-secondary mb-3 mt-2">{{data.email}}</p>
                      {{!-- <p class="text-muted font-size-sm">Bay Area, San Francisco, CA</p> --}}
                      <a href="" data-toggle="modal" data-target="#exampleModalCenter"  class="btn btn-info px-4">Edit</a>
                      <a href="/logout"  class="btn btn-outline-warning px-4">Logout</a>
                      <a href="/delete" class="btn btn-outline-danger">Delete</a>
                    </div>
                  </div>
                </div>
              </div>
                      </div>
                    </div>
                    <div class="col-md-8  ">
                      <div class="car-wrap rounded ftco-animate ">
                        
                        <div class="text">
                          <h2 class="mb-0"><a>{{name}}</a></h2>
                          <div class="d-flex mb-1">
                            <span class="cat">Email </span>
</span>
                            <p class="price ml-md-auto ml-2">{{data.email}} <span></span></p>
                          </div>
                          <div class="d-flex mb-1">
                            <span class="cat">Phone </span>
</span>
                            <p class="price ml-md-auto ml-2">{{data.phone}} <span></span></p>
                          </div>
                          <div class="d-flex mb-1">
                            <span class="cat">Age </span>
                          </span>
                            <p class="price ml-md-auto ml-2">{{data.age}} <span></span></p>
                          </div>
                          <div class="d-flex mb-1">
                            <span class="cat">License Number </span>
                            </span>
                            <p class="price ml-md-auto ml-2">{{data.licenseNumber}} <span></span></p>
                          </div>
                            <p style="color: black;" >Address</p>
                           <div class="d-flex mb-1">
                            <span class="cat">Place </span>
                            </span>
                            <p class="price ml-md-auto ml-2">{{address.place}} <span></span></p>
                          </div>
                          <div class="d-flex mb-1">
                            <span class="cat">zip </span>
                            </span>
                            <p class="price ml-md-auto ml-2">{{address.zip}} <span></span></p>
                          </div>
                          <div class="d-flex mb-1">
                            <span class="cat">House Name </span>
                            </span>
                            <p class="price ml-md-auto ml-2">{{address.houseName}} <span></span></p>
