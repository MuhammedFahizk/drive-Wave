<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">

	<link href='https://unpkg.com/boxicons@2.0.9/css/boxicons.min.css' rel='stylesheet'>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">	<!-- My CSS -->

	<!-- My CSS -->
	<link rel="stylesheet" href="/css/dashboard.css">

	<title>AdminHub</title>
</head>
<body>


	<!-- SIDEBAR -->
<section id="sidebar">
		<a href="" class="brand">
			<i class='bx bxs-smile'></i>
			<span class="text">AdminHub</span>
		</a>
		<ul style="padding: 0;" class="side-menu top">
			<li >
				<a href="/admin/DashboardPage">
					<i class='bx bxs-dashboard' ></i>
					<span class="text">Dashboard</span>
				</a>
			</li>
			<li >
				<a href="/admin/CarPage">
                    <i class='bx bxs-car' ></i>
					<span class="text">Cars</span>
				</a>
			</li>
			<li>
				<a href="/admin/Vendor">
					<i class='bx bxs-user-circle' ></i>
					<span class="text">Vendors</span>
				</a>
			</li>
			<li>
				<a href="/admin/users">
                    <i class='bx bxs-user-account'></i>
					<span class="text">Users</span>
				</a>
			</li>
			<li>
				<a href="/adminBooking">
                    <i class='bx bxs-book-bookmark'></i>
					<span class="text">Booking</span>
				</a>
			</li>
			<li>
				<a href="/adminPayment">
					<i class='bx bxs-group' ></i>
					<span class="text">Payment</span>
				</a>
			</li>
            <li class="active">
				<a href="/admin/Notification">
				<i class='bx bxs-bell' ></i>
					<span class="text">Notification</span>
				</a>
			</li>
		</ul>
		<ul style="padding: 0;" class="side-menu">
			<li>
				<a href="#">
					<i class='bx bxs-cog' ></i>
					<span class="text">Settings</span>
				</a>
			</li>
			<li>
				<a href="/admin/adminLogout" class="logout">
					<i class='bx bxs-log-out-circle' ></i>
					<span class="text">Logout</span>
				</a>
			</li>
		</ul>
	</section> 



	<!-- CONTENT -->
	<section id="content">
		<!-- NAVBAR -->
		<nav>
			<i class='bx bx-menu' ></i>
			<a href="#" class="nav-link">Categories</a>
			<form action="#">
				<div class="form-input">
					<input type="search" placeholder="Search...">
					<button type="submit" class="search-btn"><i class='bx bx-search' ></i></button>
				</div>
			</form>
			<input type="checkbox" id="switch-mode" hidden>
			<label for="switch-mode" class="switch-mode"></label>
			<a href="/admin/Notification" class="notification">
				<i class='bx bxs-bell' ></i>
				<span class="num">{{count}}</span>
			</a>
			<a href="#" class="profile">
				<img src="img/people.png">
			</a>
		</nav>
		<!-- NAVBAR -->

		<!-- MAIN -->
		<main>
			<div class="head-title">
				<div class="left">
					<h1>Dashboard</h1>
					<ul class="breadcrumb">
						<li>
							<a href="#">Dashboard</a>
						</li>
						<li><i class='bx bx-chevron-right' ></i></li>
						<li>
							<a class="active" href="#">Home</a>
						</li>
					</ul>
				</div>
				<a href="#" class="btn-download">
					<i class='bx bxs-cloud-download' ></i>
					<span class="text">Download PDF</span>
				</a>
			</div>

			<ul class="box-info">
				<li>
					<i class='bx bxs-calendar-check' ></i>
					<span class="text">
						<h3>{{NotificationCount}}</h3>
						<p>Notification</p>
					</span>
				</li>
				<li>
					<i class='bx bxs-group' ></i>
					<span class="text">
						<h3>2834</h3>
						<p>Visitors</p>
					</span>
				</li>
				<li>
					<i class='bx bxs-dollar-circle' ></i>
					<span class="text">
						<h3>$2543</h3>
						<p>Total Sales</p>
					</span>
				</li>
			</ul>
    {{#each data}}

<ul class="box-info col-md-12">
    <li>
        <div class="row col-md-12">
            <div class="col-md-7">
                <p>{{this.name}}</p>
                <p>{{email}}</p>
            </div>
            <div class="col-md-5">
                <a type="button" onclick="handleRowClickVendorNotification(this)"  data-bs-toggle="modal" class="btn btn-outline-warning mb-2" data-bs-target="#viewVendorDetails" data-vendorId='{{_id}}'>
                    Information
                </a>
                <a type="button" href="/admin/disableVendor?id={{_id}}" class="btn btn-outline-danger mb-2">
                    Disable
                </a>
                <a type="button" href="/admin/enableVendor?id={{_id}}" methode="post" class="btn btn-outline-success mb-2">
                    Enable
                </a>
            </div>
        </div>
    </li>
</ul>

                {{/each}}

				 {{#each adminDoc.notifications}}
<ul class="box-info col-md-12">
    <li>
        <div class="row col-md-12">
            <div class="col-md-9">
<p>Message: {{this.message}}</p>
{{#if userId}}
  <p>Name: {{userId.name}}</p>
  <p>Phone: {{userId.phone}}</p>
  <p>Email: {{userId.email}}</p>
{{else}}
  <p>User information not available</p>
{{/if}}
            </div>
			<div class="col-md-3">
				
                <a type="button" href="/admin/deleteUserData?id={{_id}}" class=" col-12 btn btn-outline-danger mb-2">
                    Disable
                </a>
                <a type="button" href="/admin/deleteCancelUser?id={{_id}}&&userId={{userId._id}}" methode="post" class=" col-12 btn btn-outline-success mb-2">
                    Enable
                </a>
			</div>
        </div>
    </li>
</ul>
{{/each}}

